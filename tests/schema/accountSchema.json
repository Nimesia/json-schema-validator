{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Account",
    "type": "object",
    "required": ["email","pwd"],
    "properties": {
        "id": {
            "description": "AccountId",
            "type": "integer"
        },
        "email": {
            "description": "Email",
            "type": "string",
            "minLength": 2,
            "format": "email"
        },
        "pwd": {
            "description": "Password",
            "type": "string",
            "minLength": 8
        }
    }
}
